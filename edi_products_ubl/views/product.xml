<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_product_form" model="ir.ui.view">
        <field name="model">product.catalogue</field>
        <field name="inherit_id" ref="product_catalogue.product_catalogues_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form/*" position="before">
                <header>
                   <button name="send_product_button" string="Send Catalog" class="oe_highlight" type="object"/>
                </header>
            </xpath>
        </field>
    </record>
</odoo>